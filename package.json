{
	"name": "time-to-watch",
	"version": "0.2.0",
	"private": true,
	"description": "Choose films to watch that are less than a specified running time",
	"scripts": {
		"build": "npm run tailwind && npm run eleventy"
		,"tailwind": "npx tailwindcss build ./src/css/tailwind.css -o ./src/assets/css/tailwind.css"
		,"eleventy": "npx @11ty/eleventy --input=./src/"
		,"watch": "npx @11ty/eleventy --input=./src/ --serve --watch"
	},
	"homepage": "https://wrherbert.github.io/time-to-watch#readme",
	"devDependencies": {
		"@11ty/eleventy": "^0.11.1"
	},
	"dependencies": {
		"tailwindcss": "^1.9.6"
	}
}
